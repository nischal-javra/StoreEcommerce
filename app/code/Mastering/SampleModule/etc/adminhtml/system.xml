                </field>
                <field id="cron_expression" sortOrder="20" translate="label" type="text" showInDefault="1" showInStore="0" showInWebsite="0">
                    <label>Add Item Cron Expression</label>
                </field>
            </group>
        </section>
    </system>
</config>
