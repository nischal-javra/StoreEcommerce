<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="pfay_contacts_pfay_contacts" resource="default" engine="innodb" comment="pfay_contacts_pfay_contacts Table">
        <column xsi:type="int"
                name="pfay_contacts_id"
                padding="10"
                unsigned="true"
                nullable="false"
                identity="true"
                comment="Entity Id"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="pfay_contacts_id"/>
        </constraint>
        <column name="name"
                nullable="true"
                xsi:type="varchar"
                comment="name"
                length="255"/>
        <column name="email"
                nullable="true"
                xsi:type="varchar"
                comment="email"
                length="255"/>
        <column name="comment"
                nullable="true"
                xsi:type="varchar"
                comment="comment"
                length="255"/>
    </table>
</schema>
